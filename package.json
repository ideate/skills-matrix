{
  "name": "skills-matrix",
  "version": "1.0.0",
  "description": "Skills Matrix",
  "repository": "https://github.com/cafedavid/skills-matrix",
  "scripts": {
    "build": "webpack",
    "bump-major": "node ./bin/bump.js major --commit --tag",
    "bump-minor": "node ./bin/bump.js minor --commit --tag",
    "bump-patch": "node ./bin/bump.js patch --commit --tag",
    "clean-dist": "rm -f dist/*.hot-update.js*",
    "create-default-config": "echo -n \"module.exports = {\n  apiUri: 'http://localhost/api',\n  hotModuleReloadUri: 'localhost',\n  domain: 'localhost',\n  port: 80,\n  protocol: 'http'\n}\" > config.js",
    "dev-server": "node server.js",
    "lint": "eslint --ext .js,.jsx src/ test/ ./*.js",
    "prepublish": "npm run lint"
  },
  "author": "David Johnson (https://github.com/cafedavid)",
  "license": "MIT",
  "devDependencies": {
    "ag-grid": "^4.1.5",
    "ag-grid-react": "^4.1.1",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "deep-freeze": "0.0.1",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "expect": "^1.16.0",
    "json-file-plus": "^3.3.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "react-transform-hmr": "^1.0.4",
    "redux-mock-store": "1.0.2",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "color": "^0.11.1",
    "compression": "^1.6.2",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "flexboxgrid": "^6.3.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.15.0",
    "mongodb": "^2.1.16",
    "mongoose": "^4.4.16",
    "morgan": "^1.7.0",
    "papaparse": "^4.1.2",
    "radium": "^0.17.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.4",
    "react-router": "^2.1.1",
    "react-router-redux": "^4.0.2",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.4.0",
